#include<stdio.h>
#include<math.h>
int main()
{
    int a=1,b,sum=0;
    printf("\nEnter the the number till which, \n\t the sum of squares of odd numbers is to calculated:");
    scanf("%d",&b);
    do{
        sum=sum+pow(a,2);
        a=a+2;
    }while(a<=b);
    printf("\n the sum of squares of odd numbers is:%d",sum);
    return 0;
}
